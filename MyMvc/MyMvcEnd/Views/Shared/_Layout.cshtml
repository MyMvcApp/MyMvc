<!DOCTYPE html>
<html lang="zh">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>@ViewBag.Title - 应用程序后台</title>
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/layout/css")
    @Scripts.Render("~/bundles/layout")
</head>
<body class="easyui-layout">
    <div data-options="region:'north'" style="height: 35px">
        @Html.Partial("Top")
    </div>
    <div data-options="region:'west',split:true" title="导航菜单" style="width: 175px;">
        <div id="nav" class="easyui-accordion" style="width: 230px;" border="false" fit="true">
             <div title="基础管理" data-options="iconCls:'icon-sys icon'">
        <ul>
            <li>
                <div class="">
                    <a href="javascript:void(0)" rel="~/Content/UserManage" ref="12">
                        <span class="icon icon-user">&nbsp;</span>
                        <span class="nav">员工列表</span>
                    </a>
                </div>
            </li>
             <li>
                <div class="">
                    <a href="javascript:void(0)" rel="~/PagedPeoPle/PagedPeopleEnd" ref="12">
                        <span class="icon icon-user">&nbsp;</span>
                        <span class="nav">用户列表</span>
                    </a>
                </div>
            </li>
            <li>
                <div class="">
                    <a href="javascript:void(0)" rel="~/AdminLoginLog/Index" ref="12">
                        <span class="icon icon-user">&nbsp;</span>
                        <span class="nav">管理员登录日志</span>
                    </a>
                </div>
            </li>
            <li>
                <div class="">
                    <a href="javascript:void(0)" rel="~/AdminModule/Index" ref="13">
                        <span class="icon icon-user">&nbsp;</span>
                        <span class="nav">功能模块列表</span>
                    </a>
                </div>
            </li>
            <li>
                <div class="">
                    <a href="javascript:void(0)" rel="~/AdminAuthority/Index" ref="13">
                        <span class="icon icon-user">&nbsp;</span>
                        <span class="nav">权限列表</span>
                    </a>
                </div>
            </li>
        </ul>
    </div>
        </div>
    </div>
    <div id="body" data-options="region:'center',title:''">
        @RenderBody()
    </div>
    <div data-options="region:'south',split:true" style="height: 30px;">
        @Html.Partial("Bottom")
    </div>
    <div id="mm" class="easyui-menu" style="width: 150px;">
        <div id="mm-tabupdate">刷新</div>
        <div class="menu-sep"></div>
        <div id="mm-tabclose">关闭</div>
        <div id="mm-tabcloseall">全部关闭</div>
        <div id="mm-tabcloseother">除此之外全部关闭</div>
        <div class="menu-sep"></div>
        <div id="mm-tabcloseright">当前页右侧全部关闭</div>
        <div id="mm-tabcloseleft">当前页左侧全部关闭</div>
        <div class="menu-sep"></div>
        <div id="mm-exit">退出</div>
    </div>
</body>
</html>
